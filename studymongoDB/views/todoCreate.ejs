<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <title>Document</title>
</head>
<body>
  <h1>TODO CREATE</h1>
  <input type="text" id="todoAdd" value="ㅇㅇㅇ">
  <button type="submit" id="saveTodoAdd">저장</button>
  <script>
    var todoItem;
    $('#saveTodoAdd').click((e)=>{
      e.preventDefault();
      todoItem = document.querySelector('#todoAdd').value;
      $.ajax({
        url: '/todos',
        type: 'POST',
        data: {
          todoid: 2,
          content: todoItem,
          completed: false
        },
        dataType: 'text',
        success: function(){
          console.log('저장성공!');
        }
      })
    })
    
  </script>
</body>
</html>